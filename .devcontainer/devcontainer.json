{
  "name": "ldap-explorer",
  "dockerComposeFile": "docker-compose.yml",
  "service": "node",
  "workspaceFolder": "/workspaces/ldap-explorer",
  "shutdownAction": "stopCompose",
  "extensions": [
    "dbaeumer.vscode-eslint"
  ],
  "settings": {
    "files.exclude": {
      "out": false
    },
    "search.exclude": {
      "out": true
    },
    "typescript.tsc.autoDetect": "off"
  }
}
